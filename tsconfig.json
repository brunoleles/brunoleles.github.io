{
    "compilerOptions": {
        "outDir": "./",
        "module": "es6",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "target": "es5",
        "rootDir": "./",
        "allowJs": true,
        "types": [
            "babylonjs",
        ]
    },
    "include": [
        "/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}